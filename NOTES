git config user.name "paul karayan"
git config user.email "paulkarayan@gmail.com"
git config credential.username "paulkarayan"

git remote add origin git@github.com:paulkarayan/gulfwest.git

git config core.sshCommand "ssh -i /Users/pk/.ssh/github-pk-personal"


git -c user.name="paul karayan" -c user.email="paulkarayan@gmail.com" fetch
git -c user.name="paul karayan" -c user.email="paulkarayan@gmail.com" add .
git -c user.name="paul karayan" -c user.email="paulkarayan@gmail.com" commit -m "first commit"

git -c user.name="paul karayan" -c user.email="paulkarayan@gmail.com" push -u origin main
